{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"proxy": "http://localhost:3000",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"deploy": "yarn build && rm -rf ../public/** && cp -rf ./build/** ../public",
		"test": "yarn test:integration && yarn test:unit",
		"test:gitlab-ci": "yarn test:integration-gitlab-ci && yarn test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "RAILS_ENV=test bundle exec rails db:reset && (RAILS_ENV=test bundle exec rails s &) | grep -q '3000' && playwright test ; kill $(cat ../tmp/pids/server.pid)",
		"test:integration-gitlab-ci": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@iconify-icons/mdi": "^1.2.47",
		"@iconify/svelte": "^3.1.4",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.20.4",
		"@tailwindcss/typography": "^0.5.10",
		"@types/file-saver": "^2.0.7",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"axios": "^1.6.8",
		"bits-ui": "^0.21.3",
		"clsx": "^2.1.0",
		"cmdk-sv": "^0.0.15",
		"daisyui": "^4.3.1",
		"echarts": "^5.4.3",
		"echarts-gl": "^2.0.9",
		"embla-carousel-svelte": "^8.0.2",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"file-saver": "^2.0.5",
		"formsnap": "^0.5.1",
		"fp-ts": "^2.16.5",
		"improved-yarn-audit": "^3.0.0",
		"io-ts": "^2.2.21",
		"isomorphic-fetch": "^3.0.0",
		"lucide-svelte": "^0.368.0",
		"maplibre-gl": "^4.1.1",
		"mode-watcher": "^0.3.0",
		"postcss": "^8.4.31",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"radix-icons-svelte": "^1.2.1",
		"sass": "^1.69.5",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-headless-table": "^0.18.2",
		"svelte-intersection-observer": "^0.10.1",
		"svelte-legos": "^0.2.2",
		"svelte-maplibre": "^0.8.2",
		"svelte-radix": "^1.1.0",
		"svelte-sonner": "^0.3.19",
		"sveltekit-superforms": "^2.11.0",
		"tailwind-merge": "^2.2.2",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"undici": "^5.28.4",
		"vaul-svelte": "^0.3.0",
		"vite": "^4.5.3",
		"vitest": "^0.32.2",
		"zod": "^3.22.4"
	},
	"type": "module"
}
